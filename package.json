{
  "name": "excel-cell-parser",
  "version": "1.0.1",
  "description": "Convert Excel cell name string to object of format { column: 'number', row: 'number' } and back",
  "keywords": [
    "excel",
    "cell",
    "cellname",
    "covert",
    "converter",
    "parse",
    "parser"
  ],
  "license": "MIT",
  "author": "Eugene Nikitin",
  "homepage": "http://github.com/eunikitin/excel-cell-parser.git",
  "repository": {
    "type": "git",
    "url": "http://github.com/eunikitin/excel-cell-parser.git"
  },
  "bugs": {
    "mail": "eu.nikitin@gmail.com",
    "url": "http://github.com/eunikitin/excel-cell-parser/issues"
  },
  "main": "lib/index.min.js",
  "scripts": {
    "clean": "rimraf ./.nyc_output ./.tmp ./coverage ./lib",
    "dev": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.build.development.js",
    "build": "node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.config.build.production.js",
    "test": "mocha-webpack --webpack-config webpack.config.test.js \"test/runner.js\"",
    "test:watch": "mocha-webpack --webpack-config webpack.config.test.js \"test/runner.js\" --watch",
    "cover": "nyc --reporter=lcov --reporter=text npm run test",
    "coveralls": "nyc --reporter=lcov --reporter=text npm run test && cat ./coverage/lcov.info | coveralls",
    "lint": "eslint src test",
    "prepublish": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "instrument": false,
    "sourceMap": false
  },
  "dependencies": {
    "excel-column-name": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^7.0.0",
    "fs-file-tree": "^1.0.4",
    "istanbul-instrumenter-loader": "^2.0.0",
    "mocha": "^3.3.0",
    "mocha-webpack": "^0.7.0",
    "nyc": "^10.3.0",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "webpack": "^2.4.1",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.5.4",
    "webpack-uglify-js-plugin": "^1.1.9"
  }
}
